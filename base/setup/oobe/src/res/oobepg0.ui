<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.24"/>
  <object class="GtkBox" id="page">
    <property name="visible">True</property>
    <property name="can-focus">False</property>
    <property name="orientation">vertical</property>

    <child>
      <object class="GtkLabel">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <property name="label">Welcome to Microsoft Windows</property>
        <property name="xalign">0.0</property>

        <style>
          <class name="oobe-title" />
          <class name="oobe-ml-default" />
        </style>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">False</property>
        <property name="position">0</property>
      </packing>
    </child>

    <child>
      <object class="GtkLabel">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <property name="label">
Thank you for purchasing Microsoft Windows XP.

Let's spend a few minutes setting up your computer.
        </property>
        <property name="xalign">0.0</property>
        <property name="yalign">0.0</property>

        <style>
          <class name="oobe-ml-default" />
        </style>
      </object>
      <packing>
        <property name="expand">True</property>
        <property name="fill">True</property>
        <property name="position">1</property>
      </packing>
    </child>

    <child>
      <object class="GtkLabel">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <property name="label">To continue, click Next.</property>
        <property name="xalign">0.0</property>

        <style>
          <class name="oobe-text-continue" />
        </style>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">False</property>
        <property name="position">2</property>
      </packing>
    </child>
  </object>
</interface>
